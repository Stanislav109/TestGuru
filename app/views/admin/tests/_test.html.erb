<tr>
  <td><%= test.title %></td>
  <td align="center"><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td align="center"><%= test.questions.count %></td>
  <td><%= link_to t('.actions.show'), admin_test_path(test), class: "btn btn-success" %></td>
  <td><%= link_to t('.actions.edit'), edit_admin_test_path(test), class: "btn btn-warning" %></td>
  <td>
  	<%= link_to t('.actions.delete'),
  				admin_test_path(test),
  				method: :delete,
  				data: { confirm: I18n.t("confirm.message") },
          class: "btn btn-danger" %>
  </td>
</tr>
